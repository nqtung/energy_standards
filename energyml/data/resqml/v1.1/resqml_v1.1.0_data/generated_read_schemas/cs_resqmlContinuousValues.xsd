   <!--                                                         -->
   <xsd:include schemaLocation="cs_resqmlAbstractValues.xsd" />
   <xsd:include schemaLocation="typ_dataTypesResqml.xsd" />
   <!--                                                         -->
   <xsd:complexType name="cs_resqmlContinuousValues">
      <xsd:annotation>
         <xsd:documentation>Defines the numeric arrays whose values may represent a continuous numeric space.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="resqml:cs_resqmlAbstractValues">
            <xsd:sequence minOccurs="0">
               <xsd:choice minOccurs="0">
                  <xsd:element name="doubleValues" type="resqml:doubleValues" minOccurs="0" maxOccurs="1">
                     <xsd:annotation>
                        <xsd:documentation>Defines a space delimited list of double values.</xsd:documentation>
                        <xsd:appinfo>write-constraint:@minOccurs="1"</xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="floatValues" type="resqml:floatValues" minOccurs="0" maxOccurs="1">
                     <xsd:annotation>
                        <xsd:documentation>Defines a space delimited list of float values.</xsd:documentation>
                        <xsd:appinfo>write-constraint:@minOccurs="1"</xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="longValues" type="resqml:longValues" minOccurs="0" maxOccurs="1">
                     <xsd:annotation>
                        <xsd:documentation>Defines a space delimited list of long values.</xsd:documentation>
                        <xsd:appinfo>write-constraint:@minOccurs="1"</xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="intValues" type="resqml:intValues" minOccurs="0" maxOccurs="1">
                     <xsd:annotation>
                        <xsd:documentation>Defines a space delimited list of int values.</xsd:documentation>
                        <xsd:appinfo>write-constraint:@minOccurs="1"</xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="shortValues" type="resqml:shortValues" minOccurs="0" maxOccurs="1">
                     <xsd:annotation>
                        <xsd:documentation>Defines a space delimited list of short values.</xsd:documentation>
                        <xsd:appinfo>write-constraint:@minOccurs="1"</xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="byteValues" type="resqml:byteValues" minOccurs="0" maxOccurs="1">
                     <xsd:annotation>
                        <xsd:documentation>Defines a space delimited list of byte values.</xsd:documentation>
                        <xsd:appinfo>write-constraint:@minOccurs="1"</xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:choice>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <!--                                                         -->
</xsd:schema>
