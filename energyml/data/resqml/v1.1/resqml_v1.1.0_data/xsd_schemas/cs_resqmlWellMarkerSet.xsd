	<xsd:include schemaLocation="cs_resqmlWellMarker.xsd"/>
	<!--                                                         -->
	<xsd:complexType name="cs_resqmlWellMarkerSet">
		<xsd:annotation>
			<xsd:documentation>A set of points defined in 3 dimensional space.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="wellMarker" type="resqml:cs_resqmlWellMarker" minOccurs="1" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Defines the location of one vertex that represents the 
					intersection of this feature with a particular wellboare.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!--                                                         -->
</xsd:schema>
