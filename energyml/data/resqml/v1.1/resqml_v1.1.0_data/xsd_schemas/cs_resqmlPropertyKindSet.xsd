	<xsd:include schemaLocation="cs_resqmlPropertyKind.xsd"/>
	<!--                                                         -->
	<xsd:complexType name="cs_resqmlPropertyKindSet">
		<xsd:annotation>
			<xsd:documentation>A container for property meta data.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="propertyKind" type="resqml:cs_resqmlPropertyKind" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Defines one locally defined (i.e., proprietary) property which is directly or indirectly 
					referenced back to one standard RESQML property kind.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!--                                                         -->
</xsd:schema>
