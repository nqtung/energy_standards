	<xsd:complexType name="cs_resqmlCellIndex">
		<xsd:annotation>
			<xsd:documentation>Defines the indices for one cell in a 3D grid.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="i" type="xsd:int" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The I index of the cell.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="j" type="xsd:int" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The J index of the cell.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="k" type="xsd:int" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The K index of the cell.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- A uid has been explicitly excluded from this type. -->
	</xsd:complexType>
	<!--                                                         -->
</xsd:schema>
