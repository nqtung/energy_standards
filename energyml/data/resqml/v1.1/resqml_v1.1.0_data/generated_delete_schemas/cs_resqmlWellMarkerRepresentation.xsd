   <xsd:include schemaLocation="cs_resqmlAbstractFeatureRepresentation.xsd" />
   <xsd:include schemaLocation="cs_resqmlWellMarkerPatch.xsd" />
   <!--                                                         -->
   <xsd:complexType name="cs_resqmlWellMarkerRepresentation">
      <xsd:annotation>
         <xsd:documentation>A set of well marker patches. All patches shall utilize the same CRS. This type inherits the following items: grp_resqmlTraceabilityDublinCore
         local3dCRS extensionAny extensionNameValue attgrp_uidGlobal</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="resqml:cs_resqmlAbstractFeatureRepresentation">
            <xsd:sequence minOccurs="0">
               <xsd:element name="wellMarkerPatch" type="resqml:cs_resqmlWellMarkerPatch" minOccurs="0" maxOccurs="1">
                  <xsd:annotation>
                     <xsd:documentation>One well marker patch. This logically a collection of points where each point represents the intersection of this feature with one wellbore
                     centerline.</xsd:documentation>
                     <xsd:appinfo>write-constraint:@minOccurs="1"</xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <!--                                                         -->
</xsd:schema>
