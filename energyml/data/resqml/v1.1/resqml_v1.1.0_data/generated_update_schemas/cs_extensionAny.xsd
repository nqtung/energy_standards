   <xsd:complexType name="cs_extensionAny">
      <xsd:annotation>
         <xsd:documentation>Extension Schema. The intent is to allow standard schema extensions which will validate in older software. Software can ignore any schema that it does
         not recognize except that it should be replicated in any modified copies.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence minOccurs="0">
         <xsd:any namespace="##any" processContents="lax" minOccurs="0" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Any element or attribute in any namespace. Only an Energistics SIG approved schema can utilize an Energistis namespace. Proprietary extensions
               should use a proprietary namespace. When the need for extensions are identified, please convey this need to the appropriate Energistis SIG for explicit inclusion in
               a subsequent schema version.</xsd:documentation>
            </xsd:annotation>
         </xsd:any>
      </xsd:sequence>
   </xsd:complexType>
