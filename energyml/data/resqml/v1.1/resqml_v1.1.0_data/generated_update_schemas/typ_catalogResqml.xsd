   <xsd:simpleType name="ProjectedCoordinateUom">
      <xsd:annotation>
         <xsd:documentation>The units of measure that are valid for vertical gravity based coordinates (i.e., elevation or vertical depth) within the context of a
         well.</xsd:documentation>
         <xsd:appinfo>Modified-in-version=1.4.0, By-issue=1.3.1-86, Change=Moved</xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="resqml:abstractUomEnum">
         <xsd:enumeration value="m">
            <xsd:annotation>
               <xsd:documentation>meter</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="ft">
            <xsd:annotation>
               <xsd:documentation>International Foot</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="ftUS">
            <xsd:annotation>
               <xsd:documentation>US Survey Foot</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="ftBr(65)">
            <xsd:annotation>
               <xsd:documentation>British Foot 1865</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="chBnB">
            <xsd:annotation>
               <xsd:documentation>Benoit chain (1895 B)</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="chSe">
            <xsd:annotation>
               <xsd:documentation>Sears chain</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="ftSe">
            <xsd:annotation>
               <xsd:documentation>Sears Foot</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="ydSe">
            <xsd:annotation>
               <xsd:documentation>Sears yard</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="ftCla">
            <xsd:annotation>
               <xsd:documentation>Imperial Foot. Same as EPSG "Clarke foot".</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="lkCla">
            <xsd:annotation>
               <xsd:documentation>Clarke link</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="ftGC">
            <xsd:annotation>
               <xsd:documentation>Gold Coast Foot</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="ydInd">
            <xsd:annotation>
               <xsd:documentation>Indian yard</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="km">
            <xsd:annotation>
               <xsd:documentation>kilometre</xsd:documentation>
            </xsd:annotation>
