   <xsd:simpleType name="ResqmlMostComplexLineGeometry">
      <xsd:annotation>
         <xsd:documentation>Defines the most complex geometry type used in constructing a corner point grid.</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="resqml:abstractTypeEnum">
         <xsd:enumeration value="vertical">
            <xsd:annotation>
               <xsd:documentation>This is the less complex than straight.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="straight">
            <xsd:annotation>
               <xsd:documentation>This is less complex than curved and more complex than vertical.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="curved">
            <xsd:annotation>
               <xsd:documentation>This is more complex than straight or vertical.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
