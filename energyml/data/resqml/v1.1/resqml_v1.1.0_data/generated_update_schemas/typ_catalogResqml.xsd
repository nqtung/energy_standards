   <xsd:simpleType name="GlobalAxisOrder">
      <xsd:annotation>
         <xsd:documentation>Defines the cordinate system axis order of the global CRS using the axis names (from EPSG database).</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="resqml:abstractTypeEnum">
         <xsd:enumeration value="easting northing">
            <xsd:annotation>
               <xsd:documentation>The first axis is easting and the second axis is northing.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="northing easting">
            <xsd:annotation>
               <xsd:documentation>The first axis is northing and the second asis is easting.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="westing southing">
            <xsd:annotation>
               <xsd:documentation>The first axis is westing and the second axis is southing.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="southing westing">
            <xsd:annotation>
               <xsd:documentation>The first axis is southing and the second axis is westing.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="northing westing">
            <xsd:annotation>
               <xsd:documentation>the first axis is northing and the second axis is westing.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="westing northing">
            <xsd:annotation>
               <xsd:documentation>the first axis is westing and the second axis is northing.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
