   <xsd:simpleType name="CellShape">
      <xsd:restriction base="resqml:abstractTypeEnum">
         <xsd:enumeration value="cartesian">
            <xsd:annotation>
               <xsd:documentation>Cells defined by the tri-linear isoparametric mapping from the unit cube.</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
