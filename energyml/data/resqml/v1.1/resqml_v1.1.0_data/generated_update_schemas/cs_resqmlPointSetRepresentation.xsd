   <xsd:include schemaLocation="cs_resqmlAbstractFeatureRepresentation.xsd" />
   <xsd:include schemaLocation="cs_resqmlPointSetPatch.xsd" />
   <!--                                                         -->
   <xsd:complexType name="cs_resqmlPointSetRepresentation">
      <xsd:annotation>
         <xsd:documentation>A set of point set patches. All patches shall utilize the same CRS. This type inherits the following items: grp_resqmlTraceabilityDublinCore local3dCRS
         extensionAny extensionNameValue attgrp_uidGlobal</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="resqml:cs_resqmlAbstractFeatureRepresentation">
            <xsd:sequence minOccurs="0">
               <xsd:element name="pointSetPatch" type="resqml:cs_resqmlPointSetPatch" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:documentation>One surface patch.</xsd:documentation>
                     <xsd:appinfo>write-constraint:@minOccurs="1"</xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <!--                                                         -->
</xsd:schema>
