   <xsd:complexType name="cs_resqmlTriangleSet">
      <xsd:annotation>
         <xsd:documentation>A set of triangles.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="trgl" type="resqml:triangle" minOccurs="1" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>A single triangle defined by the index of each corner vertex.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
