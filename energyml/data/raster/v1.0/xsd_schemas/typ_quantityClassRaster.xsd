	<xsd:annotation>
		<xsd:documentation>This file defines a set of standard POSC units of measure 
		for various quantity classes. This units list captures information contained 
		in the POSC units dictionary at http://www.posc.org/refs/poscUnits20.xml. 
		The enumerated lists do not contain any deprecated units. </xsd:documentation>
	</xsd:annotation>
	<!--                                                         -->
	<xsd:include schemaLocation="typ_baseType.xsd"/>
	<!--                                                         -->
	<xsd:simpleType name="FileSizeUom">
		<xsd:annotation>
			<xsd:documentation>Unit of measure of the size of a file.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="witsml:abstractUomEnum">
			<xsd:enumeration value="bytes"/>
			<xsd:enumeration value="kBytes"/>
			<xsd:enumeration value="MBytes"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ResolutionUom">
		<xsd:annotation>
			<xsd:documentation>Unit of measure for image resolution.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="witsml:abstractUomEnum">
			<xsd:enumeration value="pixels/inch"/>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>
