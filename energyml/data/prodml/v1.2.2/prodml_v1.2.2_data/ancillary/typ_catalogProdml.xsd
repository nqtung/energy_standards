					<xsd:documentation>A ground that is an area above an oilfield.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="field - group">
				<xsd:annotation>
					<xsd:documentation>An area concept that is a grouping of two or more selected petroleum fields for commercial reasons or other purposes.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="field - part">
