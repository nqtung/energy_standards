	<xsd:simpleType name="ReportingFacility" final="#all">
		<xsd:annotation>
			<xsd:documentation>A kind of facility (usage of equipment or material).
			The list of standard values is contained in the enumValuesProdml.xml file. </xsd:documentation>
		</xsd:annotation>
