	<xsd:simpleType name="ReservoirFluidKind">
		<xsd:annotation>
			<xsd:documentation>Defines the reservoir hydrocarbon fluid in broad terms of its phase behaviour.
			The list of standard values is contained in the enumValuesProdml.xml file. </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="prodml:abstractTypeEnum">
			<xsd:enumeration value="black oil">
				<xsd:annotation>
					<xsd:documentation>The reservoir fluid is predominantly oil with dissolved gas</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="volatile oil">
				<xsd:annotation>
					<xsd:documentation>The reservoir fluid is a light oil with a high proportion of dissolved gas</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="critical or near critical">
				<xsd:annotation>
					<xsd:documentation>The reservoir fluid is a light oil and gas mix which exits close to its critical point in the reservoir</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="wet gas or condensate">
				<xsd:annotation>
					<xsd:documentation>The reservoir fluid is gaseous but will yield some hydrocarbon liquid when brought to surface</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="dry gas">
				<xsd:annotation>
					<xsd:documentation>The reservoir fluid is gaseous and will yield no material hydrocarbon liquid when brought to surface</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="unknown">
				<xsd:annotation>
					<xsd:documentation>The value is not known. This value should not be used in normal situations. All reasonable attempts should be made to determine the appropriate value. Use of this value may result in rejection in some situations.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!--                                                         -->
	<xsd:simpleType name="SafetyType" final="#all">
		<xsd:annotation>
			<xsd:documentation>The type of safety issues for which a count can be defined.
			The list of standard values is contained in the enumValuesProdml.xml file. </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="prodml:abstractTypeEnum">
			<xsd:enumeration value="drill or exercise">
				<xsd:annotation>
					<xsd:documentation>The safety issue is a drill or exercise</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="fire">
				<xsd:annotation>
					<xsd:documentation>The safety issue is a fire</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="first aid">
				<xsd:annotation>
					<xsd:documentation>The safety issue required first aid</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="hazard report card">
				<xsd:annotation>
					<xsd:documentation>The safety issue is a hazard report card being completed</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="job observation">
				<xsd:annotation>
					<xsd:documentation>The safety issue is an observation being made</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="lost time incident">
				<xsd:annotation>
					<xsd:documentation>The safety issue is an incident which resulted in an injured employee who required medical treatment and could not return to work the next day.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="lost time accident">
				<xsd:annotation>
					<xsd:documentation>The safety issue is an accident resulting in lost time from work .</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="miscellaneous">
				<xsd:annotation>
					<xsd:documentation>The safety issue is in the miscellaneous category</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="near miss">
				<xsd:annotation>
					<xsd:documentation>The safety issue is an incident which, under different circumstances, could have caused injury, illness and/or major damage to/loss of assets, or harm to the environment or to a third party.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="permit with SJA">
				<xsd:annotation>
					<xsd:documentation>The safety issue is a a permit with a Safe Job Analysis.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="released to air">
				<xsd:annotation>
					<xsd:documentation>The safety issue is a release of fluids to air</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="released to water">
				<xsd:annotation>
					<xsd:documentation>The safety issue is a release of fluids to water</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="restricted work">
				<xsd:annotation>
					<xsd:documentation>The safety issue is restricted work</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="safety meeting">
				<xsd:annotation>
					<xsd:documentation>The safety issue is a safety meeting</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="sent ashore">
				<xsd:annotation>
					<xsd:documentation>The safety issue is a person being sent ashore</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="severe accident">
				<xsd:annotation>
					<xsd:documentation>The safety issue is a type of accident that may challenge safety systems at a level much higher than expected</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="sick on board">
				<xsd:annotation>
					<xsd:documentation>The safety issue is a person sick on board</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="spill or leak">
				<xsd:annotation>
					<xsd:documentation>The safety issue is a spill or leak</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="traffic accident">
				<xsd:annotation>
					<xsd:documentation>The safety issue is a traffic accident</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="total permits">
				<xsd:annotation>
					<xsd:documentation>The safety issue is a count of total permits</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="year-to-date incidents">
				<xsd:annotation>
					<xsd:documentation>The safety issue is a count of year-to-date incidents</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="unknown">
				<xsd:annotation>
					<xsd:documentation>The value is not known. This value should not be used in normal situations. All reasonable attempts should be made to determine the appropriate value. Use of this value may result in rejection in some situations.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
