				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="generator">
				<xsd:annotation>
					<xsd:documentation>A single electrical generator.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="installation">
