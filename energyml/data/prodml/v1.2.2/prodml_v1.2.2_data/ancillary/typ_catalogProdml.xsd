	<xsd:simpleType name="TestReason" final="#all">
		<xsd:annotation>
			<xsd:documentation>A reason for running a well test.
			The list of standard values is contained in the enumValuesProdml.xml file. </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="prodml:abstractTypeEnum">
			<xsd:enumeration value="initial">
