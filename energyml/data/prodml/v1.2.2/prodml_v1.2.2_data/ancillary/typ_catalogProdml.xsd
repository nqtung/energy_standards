				<xsd:annotation>
					<xsd:documentation>A single flow meter.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="flowline">
				<xsd:annotation>
					<xsd:documentation>A single flowline.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="formation">
