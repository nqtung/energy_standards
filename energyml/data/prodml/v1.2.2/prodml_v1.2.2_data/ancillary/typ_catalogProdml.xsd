				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="organizational unit">
				<xsd:annotation>
					<xsd:documentation>A logical unit of organization for production reporting.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="packer">
