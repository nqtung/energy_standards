			<xsd:enumeration value="block valve">
				<xsd:annotation>
					<xsd:documentation>A single block valve.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="bottomhole">
				<xsd:annotation>
					<xsd:documentation>A single bottomhole.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="casing">
				<xsd:annotation>
					<xsd:documentation>A production casing (steel pipe) used to seal and stabilize a wellbore.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="choke">
				<xsd:annotation>
					<xsd:documentation>A single choke.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="cluster">
				<xsd:annotation>
					<xsd:documentation>A single cluster.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="commercial entity">
