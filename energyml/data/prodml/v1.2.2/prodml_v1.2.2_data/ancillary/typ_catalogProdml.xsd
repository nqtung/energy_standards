				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="consume - fuel">
				<xsd:annotation>
					<xsd:documentation>Consumed by processing equipment.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="consume - venting">
				<xsd:annotation>
					<xsd:documentation>Released into the atmosphere.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="disposal">
				<xsd:annotation>
					<xsd:documentation>A fluid stream that contains waste fluids for disposal.</xsd:documentation>
				</xsd:annotation>
