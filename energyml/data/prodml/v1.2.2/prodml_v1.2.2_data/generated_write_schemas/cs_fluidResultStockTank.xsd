   <xsd:annotation>
      <xsd:documentation>The stockTank type is used in the fluid report. It reports the temperature of the stock tank. This may need to be filled out later with more
      information.</xsd:documentation>
   </xsd:annotation>
   <!--                                                         -->
   <xsd:include schemaLocation="typ_dataTypesProdml.xsd" />
   <!--                                                         -->
   <xsd:complexType name="cs_fluidResultStockTank">
      <xsd:sequence>
         <xsd:element name="temperature" type="prodml:thermodynamicTemperatureMeasure" minOccurs="0" maxOccurs="1" />
      </xsd:sequence>
   </xsd:complexType>
</xsd:schema>
