   <xsd:include schemaLocation="typ_catalog.xsd" />
   <xsd:include schemaLocation="cs_fiberControlLine.xsd" />
   <xsd:include schemaLocation="cs_permanentCable.xsd" />
   <xsd:include schemaLocation="cs_interventionConveyance.xsd" />
   <xsd:complexType name="cs_fiberConveyance">
      <xsd:annotation>
         <xsd:documentation>The means by which this fiber segment is conveyed into the well. A choice between a permanent, intervention or control line conveyance
         method.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence minOccurs="0">
         <xsd:choice minOccurs="0" maxOccurs="1">
            <xsd:element name="intervention" type="prodml:cs_interventionConveyance" minOccurs="0" maxOccurs="1">
               <xsd:annotation>
                  <xsd:documentation>Information on type of intervention conveyance used by the optical path.</xsd:documentation>
                  <xsd:appinfo>write-constraint:@minOccurs="1"</xsd:appinfo>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="permanent" type="prodml:cs_permanentCable" minOccurs="0" maxOccurs="1">
               <xsd:annotation>
                  <xsd:documentation>Information on type of permanent conveyance used by the optical path.</xsd:documentation>
                  <xsd:appinfo>write-constraint:@minOccurs="1"</xsd:appinfo>
               </xsd:annotation>
            </xsd:element>
            <xsd:element name="fiberControlLine" type="prodml:cs_fiberControlLine" minOccurs="0" maxOccurs="1">
               <xsd:annotation>
                  <xsd:documentation>Information regarding the control line into which a fiber cable may be pumped in order to measure a facility.</xsd:documentation>
                  <xsd:appinfo>write-constraint:@minOccurs="1"</xsd:appinfo>
               </xsd:annotation>
            </xsd:element>
         </xsd:choice>
      </xsd:sequence>
   </xsd:complexType>
</xsd:schema>
