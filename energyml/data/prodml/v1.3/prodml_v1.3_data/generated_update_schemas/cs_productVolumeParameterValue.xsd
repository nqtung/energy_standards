   <xsd:include schemaLocation="typ_dataTypesProdml.xsd" />
   <xsd:include schemaLocation="cs_curveData.xsd" />
   <xsd:include schemaLocation="cs_productVolumeAlert.xsd" />
   <xsd:include schemaLocation="cs_extensionNameValue.xsd" />
