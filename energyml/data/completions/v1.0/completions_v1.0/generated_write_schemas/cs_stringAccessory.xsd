   <xsd:include schemaLocation="cs_stringEquipment.xsd" />
   <xsd:complexType name="cs_stringAccessory">
      <xsd:annotation>
         <xsd:documentation>StringAccessories contain the stringequipment's decorative components. An accessory is the stringEquipment or Strings&#226;&#8364;&#8482; decorative
         component. An accessory is NOT directly screwed to the string. This part DOES NOT carry the weight of the rest of the String as opposed to the stringEquipment which does.
         An Accessory is UNLIKE an Assembly on which the stringEquipment is built out of.</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="accessory" type="witsml:cs_stringEquipment" maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>Information on a single accessory.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
