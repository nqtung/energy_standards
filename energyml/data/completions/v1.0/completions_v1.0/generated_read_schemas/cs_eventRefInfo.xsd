   <xsd:include schemaLocation="typ_dataTypes.xsd" />
   <xsd:complexType name="cs_eventRefInfo">
      <xsd:annotation>
         <xsd:documentation>Event reference collection</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence minOccurs="0">
         <xsd:element name="eventRefID" type="witsml:refString" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The referencing eventledger ID</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="eventDate" type="witsml:timestamp" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>install/pull date</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
